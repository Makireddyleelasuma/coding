#include<iostream>
using namespace std;
int main()
{
int a;
q>>a;
while(a--)
{
int b,arr[200]={0};
q>>b;
arr[0]=1;
int i=1,m=1,index=0,tem=0,x;
while(i<=b)
{
int k=m;
index=0;
while(k--)
{
x=arr[index]*i+temp;
arr[index]=x%10,temp=x/10,index++;
}
while(temp!=0)
{
arr[index]=temp%10;
temp=temp/10;
index++,m++;
}
i++;
}
while(m--)count<<arr[m];
count<<end1;
}
return 0;
}

