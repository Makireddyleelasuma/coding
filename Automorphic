class Automorphic
{
static boolean isAutomorphic(int num)
{
int square=num=num*num;
while(num>0)
{
if(num%10!=square%10)
return false;
num=num/10;
square=square/10;
}
return true;
}
public static void main(String args[])
{
System.out.println(isAutomorphic(76)?"Automorpic":"not Automorpic");
System.out.println(isAutomorphic(13)?"Automorpic":"not Automorpic");
}
}


OUTPUT:
D:\coding>javac Automorpic.java

D:\coding>java  Automorphic
Automorpic
Automorpic
